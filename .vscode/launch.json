{
  "version": "2.0.0",
  "configurations": [{
      "type": "node",
      "request": "attach",
      "name": "Attach Debug",
      "sourceMaps": true,
      "port": 9229,
      "restart": true,
      "skipFiles": [
        "<node_internals>/**"
      ]
    },
    {
      "name": "Run mocha debug",
      "type": "node",
      "request": "launch",
      "program": "${workspaceRoot}/node_modules/mocha/bin/_mocha",
      "stopOnEntry": false,
      "sourceMaps": true,
      "args": [
        "DOTENV_CONFIG_PATH=./.env.test"
      ],
      "cwd": "${workspaceRoot}",
      "runtimeExecutable": null,
      "runtimeArgs": [
        "--inspect",
        "--nolazy"
      ]
    }
  ]
}